{
    "name": "fcc-web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.3",
        "@fortawesome/fontawesome-free-brands": "^5.0.6",
        "classnames": "^2.5.1",
        "eeyellow.timeline": "github:eeyellow/eeyellow.Timeline",
        "react": "^18.3.1",
        "react-chrono": "^2.6.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.1",
        "swiper": "^11.1.14",
        "web-utility": "^4.4.0"
    },
    "scripts": {
        "prepare": "husky",
        "test": "lint-staged  &&  tsc --noEmit",
        "clean": "rm -rf .parcel-cache/ dist/",
        "start": "npm run clean  &&  parcel public/index.html",
        "pack": "npm run clean  &&  parcel build public/index.html --public-url .",
        "build": "npm run pack  &&  workbox generateSW"
    },
    "fcc": {
        "name": "fcc.chengdu",
        "type": "react",
        "root": "./build",
        "publicUrl": "https://fcc-cd.dev/fcc.chengdu/"
    },
    "devDependencies": {
        "@parcel/packager-raw-url": "~2.12.0",
        "@parcel/transformer-sass": "~2.12.0",
        "@parcel/transformer-webmanifest": "~2.12.0",
        "@types/jquery": "^3.5.30",
        "@types/node": "^20.16.5",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "husky": "^9.1.5",
        "lint-staged": "^15.2.10",
        "parcel": "~2.12.0",
        "prettier": "^3.3.3",
        "process": "^0.11.10",
        "typescript": "~5.5.4",
        "workbox-cli": "^7.1.0"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{html,md,scss,json,yml,js,jsx,ts,tsx}": "prettier --write"
    },
    "browserslist": "> 0.5%, last 2 versions, not dead, IE 11"
}
